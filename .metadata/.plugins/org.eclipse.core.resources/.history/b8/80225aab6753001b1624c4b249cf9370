    System.out.println("AUSWERTUNG:");
    	
    int gesamt = höchstgeschwindigkeit + beschleunigung + reichweiteVerbrenner + reichweiteAlternativ + anzahldieselmotoren + anzahlbenzinmotoren + anzahlalternativsysteme + sportmotor+ fünfgang + sechsgang + weiteregetriebe + esp + abs + assistent + allrad + alternativantriebsauswahl + co2ausstoß + schadstoffnorm + bedienung + übersicht + gestaltung + gestaltunganzeigentacho + übersichtanzeigentacho + einteilunganzeigentacho + radio + radioposition + radiobedienung + radioübersicht + klimaanlage + klimaanlageposition + klimaanlagebedienung + klimaanlageübersicht + bordpc + bordpcbedienung + bordpcübersicht + bordpcfunktionen + interiuermöglichkeiten + wechselmöglichkeiten + zusatz + weiteres + extraversion + grundpreis + aufpreise + preisleistungsverhältnis ;
    float prozentsatz = gesamt/246;
    
    int note = 0;
    
    if(gesamt > 232) {
    	note = note + 1;
    }
    if(gesamt < 232) {
		note = note + 2;
    }	
    if(gesamt < 99*2) {
    	note = note + 1;
    }
    if(gesamt < 74*2) {
    	note = note + 1;
    }
    if(gesamt < 50*2) {
    	note = note + 1;
    }
    if(gesamt < 32*2) {
    	note = note + 1;	
    }
    
    switch(note) {
    	case 1: System.out.println("Note: 1" + "; Punkte: " + gesamt); System.out.println("Prozentsatz erreicht: " + prozentsatz*100 + "%"); break;
    	case 2: System.out.println("Note: 2" + "; Punkte: " + gesamt); System.out.println("Prozentsatz erreicht: " + prozentsatz*100 + "%"); break;
    	case 3: System.out.println("Note: 3" + "; Punkte: " + gesamt); System.out.println("Prozentsatz erreicht: " + prozentsatz*100 + "%"); break;
    	case 4: System.out.println("Note: 4" + "; Punkte: " + gesamt); System.out.println("Prozentsatz erreicht: " + prozentsatz*100 + "%"); break;
    	case 5: System.out.println("Note: 5" + "; Punkte: " + gesamt); System.out.println("Prozentsatz erreicht: " + prozentsatz*100 + "%"); break;
    	case 6: System.out.println("Note: 6" + "; Punkte: " + gesamt); System.out.println("Prozentsatz erreicht: " + prozentsatz*100 + "%"); break;
    	default: System.out.println("Es ist ein Fehler bei der Notenberechnung aufgetreten!"); break;
    }

    System.out.println(" ");
    
